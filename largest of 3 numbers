#include <iostream>
using namespace std;

int main() {
   
    int a,b,c;
    cin>>a>>b>>c;
    cout<<"larger is:";
    if(a>b){
        if(a>c){
            cout<<a<<endl;
            
        }else{
            cout<<endl;
        }
    }
    else{
        if(b>c){
            cout<<b<<endl;
            
        }else{
            cout<<c<<endl;
        }
    }
    return 0;
}
